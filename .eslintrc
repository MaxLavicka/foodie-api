{
  "extends": "airbnb-base",
  "rules": {
    "func-names": ["error", "never"],
    "arrow-body-style": ["error", "as-needed"],
    "object-curly-newline": ["error", {
      "ObjectExpression": "always",
      "ObjectPattern": { "multiline": true }
    }],
    "no-console": 0,
    "no-plusplus": 0,
    "consistent-return": 0,
    "radix": 0
  },
  "env": {
    "jest": true
  }
}
